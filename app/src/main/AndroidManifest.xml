            android:name="com.recrespite.recrespite.AddParticipants"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.recrespite.recrespite.Home"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.recrespite.recrespite.Library"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.recrespite.recrespite.SelectedLibraryItem"
            android:screenOrientation="portrait" />

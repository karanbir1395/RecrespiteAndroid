            android:screenOrientation="portrait" />
        <activity
            android:name="com.recrespite.recrespite.EmptyLib"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.recrespite.recrespite.MyProfile"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.recrespite.recrespite.selectParticipant"
            android:screenOrientation="portrait" />
        <activity
